<resources>
    <style name="CustomStyle.Button.Circle.Outlined" parent="Widget.MaterialComponents.Button.OutlinedButton.Icon">
        <item name="android:textColor">@color/grey600</item>
        <item name="rippleColor">@null</item>
        <item name="android:padding">0dp</item>
        <item name="iconPadding">0dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetRight">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="shapeAppearance">@style/ShapeAppearance.MyApp.CircleButton</item>
    </style>

    <style name="LightPopup" parent="@style/ThemeOverlay.AppCompat.Light" />

    <style name="FloatingLabelTextAppearance" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
</resources>
